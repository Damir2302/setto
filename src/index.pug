        .container

            h2.s-title Домокомплекты для самостоятелной сборки

            .catalog__inner

                .catalog__item
                    a(href="#")
                        .catalog__item-image
                            img(src="assets/images/img-7.jpg", alt="img")
                        p.catalog__item-title из клееного бруса
                        p.catalog__item-price от 48 000 ₽/м³

                .catalog__item
                    a(href="#")
                        .catalog__item-image
                            img(src="assets/images/img-8.jpg", alt="img")
